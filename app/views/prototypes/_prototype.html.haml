.col-sm-4.col-md-3.proto-content
  .thumbnail
    = link_to image_tag("sample.png"), prototype_path(prototype.id), method: :get
    .caption
      %h3
        = prototype.title
      .proto-meta
        .proto-user
          = link_to "by #{prototype.user.nickname}", user_path(prototype.user.id), method: :get
        .proto-posted
          Apr 26
    %ul.proto-tag-list.list-inline
      %li
        %a{href: "#", class: "btn btn-default"} iPad
      %li
        %a{href: "#", class: "btn btn-default"} wireframe
