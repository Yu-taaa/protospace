.container.user-page
  %header.user-nav
    .media
      .media-left
        = link_to image_tag(@user.avatar, size: "64x64", class: "media-object"), user_path(@user.id),  method: :get
      .media-body
        %h4#top-aligned-media.media-heading
          Top aligned media
          %span.label.label-default.project-number
            29
            %i Projects
        .proto-user
          %span.degree
            = @user.member_name
        .row
          .col-md-6
            %h4 Profile
            %p
              = @user.profile
          .col-md-6
            %h4 Works
            %p
              = @user.works
.container.proto-list
  .row
    - @prototypes.each do |prototype|
      = render partial: "user", locals: { prototype: prototype }
.text-center
  %ul.pagination
    %li.disabled
      %a{aria: {label: "Previous", href: "#"}}
        %span{aria: {hidden: "true"}} «
    %li.active
      %a{href: "#"}
        1
        %span.sr-only (current)
    %li
      %a{href: "#"} 2
    %li
      %a{href: "#"} 3
    %li
      %a{href: "#"} 4
    %li
      %a{href: "#"} 5
    %li
      %a{aria: {label: "Next", href: "#"}}
        %span{aria: {hidden: "true"}} »
